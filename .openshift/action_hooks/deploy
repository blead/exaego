echo Creating symbolic links for configuration files
ln -sf ${OPENSHIFT_DATA_DIR}/config.json ${OPENSHIFT_REPO_DIR}/config.json
ln -sf ${OPENSHIFT_DATA_DIR}/ego/triggers.json ${OPENSHIFT_REPO_DIR}/ego/triggers.json
ln -sf ${OPENSHIFT_DATA_DIR}/plugins/memo/memo-list.json ${OPENSHIFT_REPO_DIR}/plugins/memo/memo-list.json
echo Done
